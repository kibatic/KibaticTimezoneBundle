{
    "name": "kibatic/timezone-bundle",
    "type": "symfony-bundle",
    "description": "Used to manage the display of a \\DateTimeInterface in the right timezone.",
    "keywords": [
        "timezone",
        "DateTime"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Philippe Le Van",
            "email": "philippe.levan@kibatic.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "symfony/framework-bundle": "^6.0",
        "symfony/twig-bundle": "^6.0",
        "twig/twig": "^3.0",
        "twig/intl-extra": "^3.0",
        "twig/extra-bundle": "^3.0"
    },
    "require-dev": {
        "symfony/console": "^6.0",
        "symfony/phpunit-bridge": "^5.1",
        "symfony/yaml": "^6.0",
        "symfony/translation": "^6.0",
        "symfony/browser-kit": "^6.0",
        "symfony/var-dumper": "^6.0"
    },
    "autoload": {
        "psr-4": { "Kibatic\\TimezoneBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    }
}
